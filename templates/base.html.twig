<!DOCTYPE html>

<html>

<head>

    <meta charset="UTF-8">

    <title>{% block title %}Asso{% endblock %}</title>

    {% block stylesheets %}

        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

        <link href="{{ asset('assets/css/owl.css') }}" rel="stylesheet" />

    {% endblock %}

</head>

<body>

<div>
    {% block sidebar %}
        <nav class="navbar navbar-default">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">Le Hibou qui Geek</a>
                </div>
                <ul class="nav navbar-nav">
                    <li class="active"><a href="#">Accueil</a></li>
                    <li><a href={{ path('articleFormDisplay') }}>Création article</a></li>
                    <li><a href="{{ path('articleAdmin') }}">Administration article</a></li>
                </ul>
            </div>
        </nav>

    {% endblock %}
</div>

<div class="content">

        {% block content %}

        {% endblock %}



<div style="clear:both">

    {% block footer %}

        <footer class="page-footer font-small blue">

            <!-- Copyright -->
            <div class="footer-copyright footer text-center py-3">© 2018 helvie.fr</div>
            <!-- Copyright -->

        </footer>    {#</div>#}

    {% endblock %}
</div>
</div>




{% block javascripts %}

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>
    <script src="https://cdn.ckeditor.com/ckeditor5/1...nt/ckeditor.js"></script>
    <script src="[ckeditor-build-path]/ckeditor.js"></script>

{% endblock %}

</body>

</html>

            <div class="container" style="text-align:center;margin-top:100px;">

                <h2>Nous contacter</h2>

            </div>

            <div class="container">
                <div class="formBlock contactForm1">
                    <div class="col-lg-6 col-md-6 col-sm-8 col-xs-10 col-lg-offset-3 col-md-offset-3 col-sm-offset-2 col-xs-offset-1">
                        <div class="card1">

                            <div class="card1-header card1-header-rose card1-header-icon">
                                <div class="card1-icon">
                                    <i class="material-icons" style="color:#113441">mail_outline</i>
                                </div>
                            </div>

                            <div class="card1-body ">

                                {{ form_start(formNL, {'attr': {'class': 'form-group'}} ) }}

                                <div>
                                    {{ form_widget(formNL.name, {'attr': {'required': true, 'class' : 'form-control'}}) }}
                                    {{ form_errors(formNL.name) }}
                                </div>

                                <div>
                                    {{ form_widget(formNL.email, {'attr': {'required': true, 'class' : 'form-control'}}) }}
                                    {{ form_errors(formNL.email) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.quoteRequest, {'attr': {'required': false, 'class' : 'form-control'}}) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.newsletterRequest, {'attr': {'required': false, 'class' : 'form-control'}}) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.informationsRequest, {'attr': {'required': false, 'class' : 'form-control'}}) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.archived, {'attr': {'required': false, 'class' : 'form-control'}}) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.lastAction, {'attr': {'required': false, 'class' : 'form-control'}}) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.lastActionDate, {'attr': {'required': false, 'class' : 'form-control'}}) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.nextAction, {'attr': {'required': false, 'class' : 'form-control'}}) }}
                                </div>

                                <div>
                                    {{ form_row(formNL.nextActionDate, {'attr': {'required': false, 'class' : 'form-control '}}) }}
                                </div>

                                <div class="card1-footer ">
                                    {{ form_row(formNL.save, {'attr': {'class' : 'form-control btn btn-fill btn-rose'},'label': 'Valider'}) }}
                                </div>

                                {{ form_end(formNL) }}


                            </div>



                        </div>
                    </div>
                </div>
            </div>

        </div>


